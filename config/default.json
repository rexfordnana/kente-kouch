{
  "logger": {
    "app_id": "Yeahman",
    "streams": {
      "stdout": {
        "level": "info"
      }
    }
  },
  "env": "dev",
  "dbConfig": {
    "routing": "mongodb+srv",
    "host": "@rex-mongo-cluster-hsd47.mongodb.net/",
    "dbName": "kente",
    "options": {
      "useNewUrlParser": true,
      "useUnifiedTopology": true,
      "useCreateIndex": true,
      "useFindAndModify": false,
      "autoIndex": false,
      "poolSize": 10,
      "serverSelectionTimeoutMS": 5000,
      "socketTimeoutMS": 45000,
      "family": 4
    }
  }
}